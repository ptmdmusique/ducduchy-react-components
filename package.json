{
  "name": "ducduchy-react-components",
  "version": "2.0.1",
  "license": "MIT",
  "author": "Duc Phan",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "http://ptmdmusique.github.io/ducduchy-react-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/ptmdmusique/ducduchy-react-components.git"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build-storybook": "storybook build",
    "size": "size-limit",
    "storybook": "storybook dev -p 6006",
    "deploy-storybook-only": "gh-pages -d storybook-static -r https://github.com/ptmdmusique/ducduchy-react-components -t true",
    "deploy-storybook": "npm run build-storybook && touch ./storybook-static/.nojekyll && npm run deploy-storybook-only",
    "deploy-publish": "npm publish & npm run deploy-storybook",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "scriptComments": {
    "deploy-storybook-only": "We need to bypass jekyll or gh pages will reject Vite dynamite imports https://github.com/storybookjs/storybook/issues/20564"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/ducduchy-react-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ducduchy-react-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-interactions": "^8.6.8",
    "@storybook/addon-links": "^8.6.8",
    "@storybook/blocks": "^8.6.8",
    "@storybook/builder-vite": "^8.6.8",
    "@storybook/react": "^8.6.8",
    "@storybook/react-vite": "^8.6.8",
    "@storybook/test": "8.6.8",
    "@tsconfig/recommended": "^1.0.8",
    "@types/color": "^4.2.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.11.6",
    "gh-pages": "^5.0.0",
    "glob": "^10.4.5",
    "postcss": "^8.5.3",
    "sass": "^1.86.0",
    "storybook": "^8.6.8",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-lib-inject-css": "^2.2.1"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@floating-ui/react-dom": "^2.1.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@headlessui/react": "^2.2.0",
    "@mui/lab": "6.0.0-beta.31",
    "@mui/material": "^6.4.8",
    "@mui/system": "^6.4.8",
    "@mui/x-date-pickers": "^7.28.0",
    "@storybook/addon-themes": "^8.6.8",
    "classnames": "^2.5.1",
    "color": "^4.2.3",
    "dayjs": "^1.11.13",
    "framer-motion": "^10.18.0",
    "imask": "^6.6.3",
    "nanoid": "^5.1.5",
    "react": "^19.0.0",
    "react-calendar": "^5.1.0",
    "react-colorful": "^5.6.1",
    "react-date-picker": "^11.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-paginate": "^8.3.0",
    "react-popper": "^2.3.0",
    "react-range": "^1.10.0",
    "react-select": "^5.10.1",
    "react-textarea-autosize": "^8.5.8",
    "react-use": "^17.6.0",
    "rollup-plugin-sass": "^1.15.2"
  }
}