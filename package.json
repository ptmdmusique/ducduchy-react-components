{
  "name": "ducduchy-react-components",
  "version": "1.2.4",
  "license": "MIT",
  "author": "Duc Phan",
  "main": "dist/index.js",
  "module": "dist/ducduchy-react-components.esm.js",
  "typings": "dist/index.d.ts",
  "homepage": "http://ptmdmusique.github.io/ducduchy-react-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/ptmdmusique/ducduchy-react-components.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "build-storybook": "build-storybook",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "storybook": "start-storybook -p 6006",
    "test": "dts test --passWithNoTests",
    "deploy-storybook-only": "gh-pages -d storybook-static",
    "deploy-storybook": "npm run build-storybook && npm run deploy-storybook-only"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "all",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/ducduchy-react-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ducduchy-react-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
